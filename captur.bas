10 REM captur.bas program to calculate capture area (aperture) of an antenna.
15 PRINT "INPUT BAND FREQUENCY (14.2,18.160,21.225,144 MHZ OR OTHER)";
16 INPUT F
20 FOR N=1 to 4
25 PRINT "INPUT GD THE GAIN IN DB RELATIVE TO A DIPOLE":
30 INPUT GD
40 RD=.4343*10^(GD/10)
70 LAMBDA=3*10^2/F
80 A=3.75*RD*10^4/F^2
90 IF N=1 THEN LPRINT "FREQ(MHZ) LAMBDA GAIN DB  GAIN POWER RATIO APERTURE M^2"
100 LPRINT F,LAMBDA,GD,RD,A
105 NEXT N
110 PRINT "EXIT FROM PROGRAM CAPTUR.BAS"
120 END
